{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2018",
    "lib": ["esnext", "webworker"],
    "alwaysStrict": true,
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules/", "dist/"]
}
